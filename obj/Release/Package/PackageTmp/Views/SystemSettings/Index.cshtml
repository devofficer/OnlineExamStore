@model OnlineExam.Models.Lookup

<div class="page-head">
    <h3>System Settings</h3>
    <ol class="breadcrumb">
        <li>@Html.ActionLink("Home", "Dashboard", "Account")</li>
        <li class="active">System Settings</li>
    </ol>
</div>
<div class="form-horizontal block-flat">
    <div class="row">
        <div class="col-md-6">
            @{Html.RenderAction("_Class");}
        </div>
        <div class="col-md-6">
            @{Html.RenderAction("_Category");}
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            @{Html.RenderAction("_Subject");}
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            @{Html.RenderAction("_ClassCategory");}
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            @{Html.RenderAction("_SubjectCategory");}
        </div>
    </div>
</div>