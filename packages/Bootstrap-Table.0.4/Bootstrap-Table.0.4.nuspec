<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Bootstrap-Table</id>
    <version>0.4</version>
    <title>Bootstrap Table</title>
    <authors>Frej Norling</authors>
